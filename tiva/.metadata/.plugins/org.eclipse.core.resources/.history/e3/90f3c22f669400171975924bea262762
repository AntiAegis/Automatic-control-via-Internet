/******************************************************************************
 *	Public
 *****************************************************************************/
/*
 *  Function:
 *
 *  Input   :
 *
 *  Output  :
 */
bool icdiSetup()
{
    /* Setup UART */
    uartSetup(ui32ClkFreq, ICDI_MODULE, 115200, UART_INT_OFF);

    /* Notify */
    uartPrintf("[ICDI] module is enabled.\n");
}
//-----------------------------------------------------------------------------
/*
 *  Function:
 *
 *  Input   :
 *
 *  Output  :
 */
void icdiSendChar(char ch)
{
    uartSendChar(ICDI_MODULE, ch);
}
//-----------------------------------------------------------------------------
/*
 *  Function:
 *
 *  Input   :
 *
 *  Output  :
 */
void icdiSendStr(char ch)
{
    uartSendChar(ICDI_MODULE, ch);
}
