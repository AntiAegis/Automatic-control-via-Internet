/* Project */
#include "modules/pin_def.h"
#include "system/system.h"


/******************************************************************************
 *  Definition
 *****************************************************************************/


/******************************************************************************
 *  Main
 *****************************************************************************/
void main()
{
    /* Setup */
//    systemSetup();
    fastSetup(1, 1);

    /* Serve */
    while(1)
    {
        uartSendStr(UART_ICDI, "Nguyen Chinh Thuy\n");
        gpioLEDState(1, 2);
        clkDelayMs(1000);
    }
}


